#include<stdio.h>
#include<string.h>


int main()
{
    
    char g[100];
  
    
    const char s[4] = "-";
    char* tok;
    printf("accept string\n");
    scanf("%s",&g);
  
    
    tok = strtok(g, s);
  
    
    while (tok != 0) {
        printf(" %s\n", tok);
  
        
        tok = strtok(0, s);
    }
  
    return 0;
}
